@model rentcar

<form method="post">
    <div class="border p-3 mt-4">
        <div class="row pb-2">
            <h2 class="text-primary">Create rentcar</h2>
            <hr />
        </div>
        @* <div asp-validation-summary="All"></div>*@
        <div class="mb-3">
            <label asp-for="Model"></label>
            <input asp-for="Model" class="form-control" />
            <span asp-validation-for="Model" class="text-danger"></span>
        </div>
        <div class="mb-3">
            <label asp-for="Price"></label>
            <input asp-for="Price" class="form-control" />
            <span asp-validation-for="Price" class="text-danger"></span>
        </div>
        <div class="mb-3">
            <label asp-for="Year"></label>
            <input asp-for="Year" class="form-control" />
            <span asp-validation-for="Year" class="text-danger"></span>
        </div>
        <div class="mb-3">
            <label asp-for="Rentdate"></label>
            <input asp-for="Rentdate" class="form-control" />
            <span asp-validation-for="Rentdate" class="text-danger"></span>
        </div>
        <div class="mb-3">
            <label asp-for="Returndate"></label>
            <input asp-for="Returndate" class="form-control" />
            <span asp-validation-for="Returndate" class="text-danger"></span>
        </div>
        <div class="mb-3">
            <label asp-for="IsAvailable"></label>
            <input asp-for="IsAvailable" class="form-control" />
            <span asp-validation-for="IsAvailable" class="text-danger"></span>
        </div>
        <button type="submit" class="btn btn-primary" style="width:150px">Create</button>
        <a asp-controller="rentcar" asp-action="Index" class="btn btn-secondary" style="width:150px">
            Back To List
        </a>
    </div>
</form>
@section Scripts{
    @{

        <partial name="_ValidationScriptsPartial" />
    }
}